package arraysDemo;

public class Main {

	public static void main(String[] args) {
		String ogrenci1 = "Ali";
		String ogrenci2 = "Ediz";
		String ogrenci3 = "Ýlkim";
		
		String [] ogrenciler = new String[3];
		ogrenciler[0] = "Ali";
		ogrenciler[1] = "Ediz";
		ogrenciler[2] = "Ýlkim";
		
		for (int i = 0; i < ogrenciler.length; i++) {
			System.out.println(ogrenciler[i]);
		}

	}

}
